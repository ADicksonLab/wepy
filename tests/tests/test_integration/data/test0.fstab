# /etc/fstab: static file system information.
#
# Use 'blkid' to print the universally unique identifier for a
# device; this may be used with UUID= as a more robust way to name devices
# that works even if disks are added and removed. See fstab(5).
#
# <file system> <mount point>   <type>  <options>       <dump>  <pass>
# / was on /dev/sda2 during installation
UUID=ASDOINADNSON /               ext4    errors=remount-ro 0       1
# /boot/efi was on /dev/sda1 during installation
UUID=SANSDNIONION  /boot/efi       vfat    umask=0077      0       1
/swapfile                                 none            swap    sw              0       0
UUID=ASFASIDUSADKNS /media/shmo/allegheny ntfs noauto,user,permissions,x-gvfs-show,x-gvfs-name=allegheny 0 0
UUID=SDFSDUINOIDNIOW /media/shmo/buckthorn auto noauto,user,permissions,x-gvfs-show,x-gvfs-name=buckthorn 0 0
UUID=SAIDOJANSA /media/shmo/camellia auto noauto,user,permissions,x-gvfs-show,x-gvfs-name=camellia 0 0
UUID=AOSNDOISNDIONSW /media/shmo/rhododendron auto noauto,x-gvfs-show,x-gvfs-name=rhododendron 0 0
LABEL=birch /media/shmo/birch auto user,permissions,nosuid,nodev,nofail,noauto,x-gvfs-show,x-gvfs-name=birch 0 0
